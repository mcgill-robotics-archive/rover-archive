<launch>

<arg name="config_file" />

<include file="$(find rover_camera)/launch/usb.launch">
    <arg name="name" value="omnicam" />
    <arg name="device" value="/dev/omnicam" />
    <arg name="decimate_factor" value="1" />
</include>

<group ns="david">
    <param name="config_file" value="$(arg config_file)" />    
    <node name="dewarp1" pkg="omnicam" type="unwrap.py" respawn="false"/>>
    
    <node name="ServiceCropTest" pkg="omnicam" type="crop_server.py" respawn="true"/>
</group>

</launch>
